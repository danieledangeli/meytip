<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Countries" table="countries">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="ccode" type="string" column="ccode" length="15" nullable="true"/>
    <field name="country" type="string" column="country" length="30" nullable="true"/>
    <field name="lastVerified" type="string" column="last_verified" length="30" nullable="true"/>
    <field name="alias" type="string" column="alias" length="30" nullable="true"/>
  </entity>
</doctrine-mapping>

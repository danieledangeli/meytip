<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="GsmEvents" table="gsm_events">
    <id name="eventid" type="integer" column="eventid">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="eventType" type="string" column="event_type" length="30" nullable="true"/>
    <field name="matchid" type="integer" column="matchid" nullable="true"/>
    <field name="gsmid" type="integer" column="gsmid" nullable="true"/>
    <field name="code" type="string" column="code" length="30" nullable="true"/>
    <field name="name" type="string" column="name" length="50" nullable="true"/>
    <field name="person" type="string" column="person" length="100" nullable="true"/>
    <field name="personId" type="integer" column="person_id" nullable="true"/>
    <field name="teamId" type="integer" column="team_id" nullable="true"/>
    <field name="gsmTeamId" type="integer" column="gsm_team_id" nullable="true"/>
    <field name="minute" type="integer" column="minute" nullable="true"/>
    <field name="minuteExtra" type="integer" column="minute_extra" nullable="true"/>
    <field name="lastUpdated" type="datetime" column="last_updated" nullable="true"/>
  </entity>
</doctrine-mapping>

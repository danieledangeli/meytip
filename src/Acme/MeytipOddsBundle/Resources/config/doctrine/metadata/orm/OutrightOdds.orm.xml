<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="OutrightOdds" table="outright_odds">
    <id name="uniqueid" type="string" column="uniqueid" length="15">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="bid" type="integer" column="bid" nullable="true"/>
    <field name="compname" type="string" column="compname" length="30" nullable="true"/>
    <field name="compid" type="integer" column="compid" nullable="true"/>
    <field name="o1" type="float" column="o1" nullable="true"/>
    <field name="marketid" type="integer" column="marketid" nullable="true"/>
    <field name="lastUpdated" type="datetime" column="last_updated" nullable="true"/>
  </entity>
</doctrine-mapping>

<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="StdoddsMoves" table="stdodds_moves">
    <id name="offerId" type="integer" column="offer_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="bid" type="integer" column="bid" nullable="true"/>
    <field name="hcapgroup" type="string" column="hcapgroup" length="20" nullable="true"/>
    <field name="lastUpdated" type="datetime" column="last_updated" nullable="false"/>
    <field name="sportid" type="integer" column="sportid" nullable="true"/>
    <field name="o1" type="float" column="o1" nullable="true"/>
    <field name="o2" type="float" column="o2" nullable="true"/>
    <field name="o3" type="float" column="o3" nullable="true"/>
  </entity>
</doctrine-mapping>
